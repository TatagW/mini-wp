s/thirdPage.vue":"<template>\n    <div>\n        <navbar \n        @publish=\"publish\"\n        @gotofifthpage=\"gotofifthpage\"\n        @gotofourthpage=\"gotofourthpage\"\n        @gotosecondpage=\"gotosecondpage\" \n        :page=\"page\" \n        @gotofirstpage=\"gotofirstpage\"></navbar>\n        <div class=\"editor\">\n            <contentwrite @updatecontent=\"updatecontent\" :title.sync=\"title\"></contentwrite>\n        </div>\n        \n    </div>\n</template>\n\n<script>\nlet baseUrl = 'http://localhost:3000'\nimport axios from 'axios'\nimport navbar from './navbar'\nimport contentwrite from './contentWrite'\nexport default {\n    props: ['page'],\n    data: function(){\n        return {\n            title: \"\",\n            content: \"\"\n        }\n    },\n    components: {\n        navbar,\n        contentwrite\n    },\n    methods: {\n        gotofourthpage(){\n            this.$emit('gotofourthpage')\n        },\n        gotofifthpage(){\n            this.$emit('gotofifthpage')\n        },\n        gotofirstpage(){\n            this.$emit('gotofirstpage')\n        },\n        gotosecondpage(){\n            this.$emit('gotosecondpage')\n        },\n        publish(publish){\n            axios({\n                url: `${baseUrl}/articles`,\n                method: 'post',\n                data: {\n                    title: this.title,\n                    content: this.content,\n                    publish\n                },\n                headers: {\n                    token: localStorage.getItem('token')\n                }\n            })\n            .then(response => {\n               console.log(response)\n            })\n            .catch(err =>{\n                if(err.response){\n                    console.log(`${err.response.data.message}`)\n                }\n                else if(err.request){\n                    alert(\"No response from server\")\n                }\n                else {\n                    console.log(err)\n                }\n            })\n            .finally(()=>{\n\n            })\n        },\n        updatecontent(value){\n            this.content = value\n        }\n    }\n}\n</script>\n\n<style scoped>\n    .editor{\n        margin-top: 5vh;\n        width: 100%;\n        display: flex;\n        justify-content: center;\n    }\n</style>"},"lineCount":null}},"error":null,"hash":"6b6d5bc330a231a60204474322b247fb","cacheData":{"env":{}}}